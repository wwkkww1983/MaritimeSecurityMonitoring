<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--目标颜色-->
  <targetColor>
    <!--工作船-->
    <workShip>#FFFE4EB9</workShip>
    <!--商船-->
    <merchantShip>#FF4F9AFF</merchantShip>
    <!--渔船-->
    <fishingShip>#FF0063D1</fishingShip>
    <!--越南渔船-->
    <vietnamShip>#FF0CFF8A</vietnamShip>
    <!--不明目标-->
    <unknown>#FFD2FE29</unknown>
    <!--尾迹线宽颜色-->
    <wakeWidth>#FF00C439</wakeWidth>
    <!--尾迹半径颜色-->
    <wakeRadius>#FFFFE50A</wakeRadius>
  </targetColor>
  <!--半径-->
  <radius>
    <!--驱逐区-->
    <expulsionZone>15</expulsionZone>
    <!--警戒区-->
    <warningArea>20</warningArea>
    <!--光电识别-->
    <photoelectric>25</photoelectric>
    <!--雷达探测识别-->
    <radar>30</radar>
  </radius>
  <!--融合参数-->
  <fusion>
    <!--xyz坐标参数-->
    <xyz>1</xyz>
    <!--abr距离门限-->
    <abrDistance>2</abrDistance>
    <!--abr角度门限-->
    <abrAngle>3</abrAngle>
    <!--雷达消失时间-->
    <radarMiss>4</radarMiss>
    <!--AID消失时间-->
    <AISMiss>5</AISMiss>
    <!--IM-->
    <IM>1</IM>
    <!--IN-->
    <IN>1</IN>
  </fusion>
  <!--IP地址-->
  <ipAddr>
    <!--数据服务器-->
    <dataSer>192.168.10.107</dataSer>
    <!--融合服务器-->
    <fusionSer>192.168.10.108</fusionSer>
    <!--监控平台1-->
    <monitor1>192.108.10.109</monitor1>
    <!--监控平台2-->
    <monitor2>92.108.10.134</monitor2>
  </ipAddr>
  <!--串口参数-->
  <serialPort>
    <!--波特率-->
    <baudRate>10</baudRate>
    <!--数据位-->
    <dataBits>10</dataBits>
    <!--奇偶校验位-->
    <parityBit>1</parityBit>
    <!--停止位-->
    <stopBit>5</stopBit>
  </serialPort>
  <!--长度-->
  <length>
    <!--比例尺-->
    <proportion>1</proportion>
    <!--尾迹线宽-->
    <lineWidth>2</lineWidth>
    <!--尾迹半径-->
    <lineRadius>10</lineRadius>
  </length>
  <!--海图信息-->
  <chartInfo>
    <!--原始比例尺-->
    <measuring>52000</measuring>
    <!--版本号-->
    <edition>1</edition>
    <!--升级版本号-->
    <upgradedVersion>0</upgradedVersion>
    <!--内存需求-->
    <memory>6525</memory>
  </chartInfo>
  <!--海图范围-->
  <chartRange>
    <!--左边-->
    <leftBoundary>60度46.00分E</leftBoundary>
    <!--右边-->
    <rightBoundary>61度20.00分E</rightBoundary>
    <!--上边-->
    <topBoundary>32度19.00分W</topBoundary>
    <!--下边-->
    <bottomBoundary>32度38.00分W</bottomBoundary>
  </chartRange>
  <!--融合参数按钮-->
  <fusionRadio>
    <!--提升-->
    <prompt>True</prompt>
    <!--告警-->
    <warn>False</warn>
    <!--最优-->
    <optimal>True</optimal>
    <!--加权-->
    <weighted>False</weighted>
    <!--SF-->
    <SF>False</SF>
  </fusionRadio>
  <!--目标显示方式-->
  <targetShow>
    <!--折线-->
    <brokenLine>True</brokenLine>
    <!--点线-->
    <pointLine>False</pointLine>
    <!--目标尾迹-->
    <targetWake>False</targetWake>
    <!--标牌显示-->
    <signDisplay>False</signDisplay>
    <!--位置-->
    <position>True</position>
    <!--速度-->
    <speed>True</speed>
    <!--加速度-->
    <acceleration>False</acceleration>
    <!--敌我属性-->
    <IFF>False</IFF>
    <!--识别属性-->
    <recognitionProperties>False</recognitionProperties>
    <!--船舶标示-->
    <shippingMark>False</shippingMark>
    <!--航首页-->
    <shipPage>False</shipPage>
    <!--呼叫号-->
    <callNumber>False</callNumber>
    <!--航迹线-->
    <trackLine>False</trackLine>
  </targetShow>
</configuration>